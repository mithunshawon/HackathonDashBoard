<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="cache-control" content="no-cache" />
    <!-- Favicon -->
    <link rel="shortcut icon" href="favicon.ico">

    <title>Hackathon</title>
    @Styles.Render("~/Content/css")
   

    <!-- CSS Global Compulsory -->
   @* <link rel="stylesheet" href="~/Content/plugins/bootstrap/css/bootstrap.min.css">*@
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/css/all.css">

    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="~/Content/plugins/line-icons/line-icons.css">
    <link rel="stylesheet" href="~/Content/plugins/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/plugins/sky-forms/version-2.0.1/css/custom-sky-forms.css">
    <link rel="stylesheet" href="~/Content/plugins/scrollbar/src/perfect-scrollbar.css">

    <!-- CSS Page Style -->
    <link rel="stylesheet" href="~/Content/css/pages/profile.css">

    <!-- CSS Theme -->
    <link rel="stylesheet" href="~/Content/css/themes/default.css" id="style_color">
    <link rel="stylesheet" href="~/Content/css/pages/feature_timeline2.css">

    <!-- CSS Customization -->
    <link rel="stylesheet" href="~/Content/css/custom.css">
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    <base href="http://localhost:49916/" />
</head>
<body ng-app="routerApp">
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Hackathon Dashboard", "", "", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a ui-sref="home">Home</a></li>
                    <li><a ui-sref="teamDetails">Team Detail</a></li>
                </ul>
                <p class="nav navbar-text navbar-right">Hello, @User.Identity.Name!</p>
            </div>
        </div>
    </div>
    <div class="container body-content">
        
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - IMS Health</p>
        </footer>
    </div>
    
    
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/signalR")
    <script src="/signalr/hubs"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.js"></script>*@
    @Scripts.Render("~/bundles/angular")
    @RenderSection("scripts", required: false)
    
</body>
</html>
   

